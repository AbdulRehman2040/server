{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "functions": {
    "api/*.js": {
      "includeFiles": [
        "node_modules/fast-cli/**",
        "node_modules/exec/**",
        "node_modules/which/**"
      ]
    }
  }
}